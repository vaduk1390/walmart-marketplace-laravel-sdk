{
    "name": "indeximstudio/walmart-marketplace-laravel-sdk",
    "description": "Connect to the Walmart Marketplace API using Laravel",
    "keywords": ["laravel", "walmart", "seller", "marketplace"],
    "license": "MIT",
    "authors": [
        {
            "name": "Keith Brink",
            "email": "keith.brink@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "KeithBrink\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KeithBrink\\Walmart\\WalmartServiceProvider"
            ],
            "aliases": {
                "WalmartMkt": "KeithBrink\\Walmart\\WalmartFacade"
            }
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.2",
        "nesbot/carbon": "^2.62.1"
    }
}
